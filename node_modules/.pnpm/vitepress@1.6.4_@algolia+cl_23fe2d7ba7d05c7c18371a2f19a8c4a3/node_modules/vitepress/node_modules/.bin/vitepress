#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Python/aikb/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_23fe2d7ba7d05c7c18371a2f19a8c4a3/node_modules/vitepress/bin/node_modules:/mnt/c/Python/aikb/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_23fe2d7ba7d05c7c18371a2f19a8c4a3/node_modules/vitepress/node_modules:/mnt/c/Python/aikb/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_23fe2d7ba7d05c7c18371a2f19a8c4a3/node_modules:/mnt/c/Python/aikb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Python/aikb/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_23fe2d7ba7d05c7c18371a2f19a8c4a3/node_modules/vitepress/bin/node_modules:/mnt/c/Python/aikb/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_23fe2d7ba7d05c7c18371a2f19a8c4a3/node_modules/vitepress/node_modules:/mnt/c/Python/aikb/node_modules/.pnpm/vitepress@1.6.4_@algolia+cl_23fe2d7ba7d05c7c18371a2f19a8c4a3/node_modules:/mnt/c/Python/aikb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
